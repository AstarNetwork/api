{
    "name": "@plasm/types",
    "version": "1.5.0",
    "description": "Plasm Network node type definitions generated with @polkadot/typegen",
    "type": "commonjs",
    "keywords": [
        "plasm network",
        "typescriipt",
        "polkadot",
        "blockchain",
        "substrate",
        "types"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PlasmNetwork/plasm-types.git"
    },
    "author": "Stake Technologies <devops@stake.co.jp>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/PlasmNetwork/plasm-types/issues"
    },
    "homepage": "https://github.com/PlasmNetwork/plasm-types#readme",
    "main": "index.js",
    "scripts": {
        "build": "ts-node -r tsconfig-paths/register scripts/build.ts",
        "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package @plasm/types --input ./src",
        "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package @plasm/types --endpoint ws://localhost:9944 --output ./src",
        "test-log": "echo \"Hello World\"",
        "lint": "tsc --noEmit --pretty"
    },
    "dependencies": {
        "@polkadot/api": "^4.9.2",
        "cross-env": "7.0.3"
    },
    "devDependencies": {
        "@polkadot/typegen": "^4.9.2",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.4"
    }
}
